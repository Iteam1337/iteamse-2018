{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "lint": "eslint src",
    "lint:watch": "esw --watch --clear src",
    "lint:fix": "eslint --fix src",
    "flow": "flow",
    "flow:watch": "flow-watch",
    "schema": "apollo-codegen introspect-schema http://localhost:4000 --output ./src/schema.json",
    "types": "apollo-codegen generate './src/**/*.js' --schema ./src/schema.json --addTypename --target flow-modern",
    "cypress": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.11",
    "apollo-client": "2.2.7",
    "apollo-link": "1.2.1",
    "apollo-link-http": "1.5.3",
    "apollo-server-express": "1.3.2",
    "contrast": "^1.0.1",
    "express": "4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "2.8.0",
    "node-fetch": "2.1.1",
    "raf": "3.4.0",
    "razzle": "0.8.14",
    "react": "16.2.0",
    "react-apollo": "2.1.0",
    "react-dom": "16.2.0",
    "react-lazyload": "^2.3.0",
    "react-markdown": "^3.3.0",
    "react-router-dom": "4.2.2",
    "react-safe": "1.1.1",
    "serialize": "0.1.3",
    "styled-components": "3.2.3",
    "what-input": "^5.0.5"
  },
  "devDependencies": {
    "apollo-codegen": "0.19.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-react-remove-properties": "0.2.5",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-export-default-name": "2.0.4",
    "cypress": "^2.1.0",
    "eslint": "4.19.1",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-react": "7.7.0",
    "eslint-watch": "3.1.3",
    "flow-bin": "0.68.0",
    "flow-watch": "1.1.3"
  }
}
